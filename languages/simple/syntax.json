{
	"CONFIG": {
		"name": "B3",
		"main": "P_LIST",
		"version": "0.1",
		"prepend_with": "",
		"append_with": ""
	},
	"SYNTHAX": {
		"PHRASES": {
			"P_LIST": {
				"or+": ["P_EXPRESSION_OR_SCOPE", "P_LIST"],
				"call": false
			},
			"P_EXPRESSION_OR_SCOPE": {
				"or": ["P_SCOPE", "P_EXPRESSION"]
			},
			"P_SCOPE": {
				"and": ["W_SCOPE_OPEN", "P_EXPRESSION", "W_SCOPE_CLOSE"]
			},
			"P_EXPRESSION": {
				"or": ["P_ABC", "P_QBA", "P_AQB", "P_ABQ", "P_A_QQ", "P_QQ_C"]
			},


			"P_ABC": {
				"and": ["P_THING_OR_SCOPE", "P_THING_OR_SCOPE", "P_THING_OR_SCOPE"],
				"call": true
			},
			"P_QBA": {
				"and": ["W_QUESTION", "P_THING_OR_SCOPE", "P_THING_OR_SCOPE"],
				"call": true
			},
			"P_AQB": {
				"and": ["P_THING_OR_SCOPE", "W_QUESTION", "P_THING_OR_SCOPE"],
				"call": true
			},
			"P_ABQ": {
				"and": ["P_THING_OR_SCOPE", "P_THING_OR_SCOPE", "W_QUESTION"],
				"call": true
			},
			"P_QQC": {
				"and": ["W_QUESTION", "W_QUESTION", "P_THING_OR_SCOPE"],
				"call": true
			},
			"P_AQQ": {
				"and": ["P_THING_OR_SCOPE", "W_QUESTION", "W_QUESTION"],
				"call": true
			},

			"P_SQQ": {
				"and": ["W_SCOPE_OPEN", "P_QQ", "W_SCOPE_CLOSE"]
			},
			"P_QQ": {
				"or": ["P_QQC", "P_AQQ"]
			},

			"P_A_QQ": {
				"and": ["P_THING_OR_SCOPE", "P_SQQ"]
			},
			"P_QQ_C": {
				"and": ["P_SQQ", "P_THING_OR_SCOPE"]
			},

			"P_THING_OR_SCOPE": {
				"or": ["P_THING", "P_SCOPE"]
			},
			"P_THING": {
				"or": ["W_THING"],
				"call": true
			}
		},
		"WORDS": {
			"W_THING":{
				"syllables": ["S_VALUE"],
				"call": true
			},
			"W_QUESTION": {
				"syllables": ["S_QUESTION"],
				"call": true
			},
			"W_SCOPE_OPEN": {
				"syllables": ["S_SCOPE_OPEN"]
			},
			"W_SCOPE_CLOSE": {
				"syllables": ["S_SCOPE_CLOSE"]
			}
		},
		"SYLLABLES": {
			"S_QUESTION": {
				"letters": "L_QUESTION",
				"max": 1
			},
			"S_VALUE": {
				"letters": "L_ALPHANUMERIC",
				"max": 255
			},
			"S_SCOPE_OPEN": {
				"letters": "L_SCOPE_OPEN",
				"max": 1
			},
			"S_SCOPE_CLOSE": {
				"letters": "L_SCOPE_CLOSE",
				"max": 1
			}
		},
		"LETTERS": {
			"L_QUESTION": "?",
			"L_ALPHANUMERIC": "01234567abcdefghigklmnopqrstuvwxyz",
			"L_SCOPE_OPEN": "(",
			"L_SCOPE_CLOSE": ")"
		}
	}
}